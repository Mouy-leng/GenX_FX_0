{
  "name": "genx-fx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "genx-cli": "genx-cli/cli.js"
  },
  "scripts": {
    "dev": "concurrently \"npm:client\" \"npm:server\" \"npm:python:dev\"",
    "client": "vite --host 0.0.0.0 --port 5173",
    "server": "tsx watch services/server/index.ts",
    "python:dev": "uvicorn api.main:app --host 0.0.0.0 --port 8000 --reload",
    "start": "tsx services/server/index.ts",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "test": "vitest"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.44.5",
    "embla-carousel-react": "^8.0.0",
    "express": "^5.1.0",
    "firebase-tools": "^14.12.1",
    "lucide-react": "^0.400.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-router-dom": "^6.15.0",
    "recharts": "^3.1.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.0.0",
    "zod": "^4.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^5.0.3",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "globals": "^15.0.0",
    "postcss": "^8.4.24",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.42.0",
    "vite": "^5.0.0",
    "vitest": "^3.2.4",
    "ws": "^8.18.3"
  }
}
